<theme>
    <formatVersion>7</formatVersion>
	
	<include subset="systemview" name="Wheel Right">./_inc/systemview/wheeldroite.xml</include>
	<include subset="systemview" name="Wheel Left">./_inc/systemview/wheelgauche.xml</include>
	<include subset="systemview" name="Horizontal">./_inc/systemview/normal.xml</include>
	<include subset="systemview" name="Vertical Right">./_inc/systemview/verticaldroit.xml</include>
	<include subset="systemview" name="Vertical Left">./_inc/systemview/verticalgauche.xml</include>	

	<include subset="infoset" name="None" displayName="${none}">./_inc/infoview/none.xml</include>	
	<include subset="infoset" name="Lower Left" displayName="${blue}">./_inc/infoview/right.xml</include>
	<include subset="infoset" name="Lower Right" displayName="${red}">./_inc/infoview/left.xml</include>
	<include subset="infoset" name="Upper Center" displayName="${green}">./_inc/infoview/mid.xml</include>
	
	<include subset="colorset" name="blue" displayName="${blue}">./_inc/colors/blue.xml</include>
	<include subset="colorset" name="red" displayName="${red}">./_inc/colors/red.xml</include>
	<include subset="colorset" name="green" displayName="${green}">./_inc/colors/green.xml</include>
</theme>
